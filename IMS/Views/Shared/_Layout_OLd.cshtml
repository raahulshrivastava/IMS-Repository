<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <!-- Main Header -->
        @Html.Partial("_Header")

        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("_SideMenu")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper" style="overflow-y: hidden;min-height:unset">
            <!-- Main content -->
            <section class="content container-fluid">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>

        <!-- Footer -->
        @Html.Partial("_Footer")

        <!-- Control Sidebar -->
        @Html.Partial("_ControlSidebar")
    </div>
    <script type="text/javascript">
        function exported(typeId) {
            $.ajax({
                type: "Post",
                url: '@Url.Action("ExportExcel", "ImportExportDocument")?type=' + typeId,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    var response = JSON.parse(data);
                    window.location = '/ImportExportDocument/Download?fileGuid=' + response.FileGuid
                                      + '&filename=' + response.FileName;
                }
            });
        }
        $(document).ready(function () {
            if ('@TempData["Message"]' != null && '@TempData["Message"]' != "") {
                $.notify({ message: '@TempData["Message"]' }, { type: 'success' });
            }

        });
    </script>
</body>
