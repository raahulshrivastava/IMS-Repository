<body class="hold-transition register-page">
    @using (Ajax.BeginForm("Registration", "Web", null, new AjaxOptions() { OnSuccess = "InsertUpdateResponse" }))
    {

        <div class="register-box">
            <div class="register-logo">
                <b>IMS</b>
            </div>
            <div class="register-box-body">
                <p class="login-box-msg">Register a new membership</p>

                <div class="form-group has-feedback">
                    @Html.TextBoxFor(a => a.FirstName, new { @type = "text", @class = "form-control", @placeholder = "First Name", @required = "required" })
                    <span class="glyphicon glyphicon-user form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    @Html.TextBoxFor(a => a.LastName, new { @type = "text", @class = "form-control", @placeholder = "Last Name", @required = "required" })
                    <span class="glyphicon glyphicon-user form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">

                    @Html.TextBoxFor(a => a.Email, new { @type = "email", @class = "form-control", @placeholder = "Email", @required = "required" })
                    <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                </div>

                <div class="form-group has-feedback">

                    @Html.TextBoxFor(a => a.ContactNumber, new { @type = "tel", @class = "form-control", @placeholder = "Contact Number", @required = "required" })
                    <span class="glyphicon glyphicon-phone form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">

                    @Html.TextBoxFor(a => a.Pasword, new { @type = "password", @class = "form-control", @placeholder = "Password", @required = "required" })
                    <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">

                    @Html.TextBoxFor(a => a.ConfirnmPasword, new { @type = "password", @class = "form-control", @placeholder = "Confirm Password", @required = "required" })
                    <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                </div>
                @*<div class="row">
                        <div class="col-xs-8">
                            <div class="checkbox icheck">
                                <label>
                                    @Html.CheckBoxFor(a => a.Agreement) I agree to the terms
                                </label>
                            </div>
                        </div>
                    <!-- /.col -->

                    <!-- /.col -->
                    </div>*@
                <div class="col-xs-4">
                    <button type="submit" class="btn btn-primary btn-block btn-flat">Register</button>
                </div>



                <a href="@Url.Action("Index","Web")" class="text-center">I already have a membership</a>
            </div>
            <!-- /.form-box -->
        </div>

    }
</body>
