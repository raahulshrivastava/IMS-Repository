
@{
    ViewBag.Title = "Stock In";
}
@using IMS.Utilities;
<div class="content-header clearfix">
    <h1 class="pull-left">
        Stock In
    </h1>
    <div class="pull-right">
       
        <a href="@Url.Action("AddEditStockInMaster", "StockIn")" type="button" class="btn bg-blue " id="btnAdd"><i class="fa fa-plus"></i>  Add</a>
